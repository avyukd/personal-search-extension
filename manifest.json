{
    "name": "NavSearch extension.",
    "description": "Easily index personal information for search.",
    "version": "1.0",
    "manifest_version": 2,
    "permissions": [
        "contextMenus",
        "https://*/",
        "http://127.0.0.1:8000/*",
        "tabs"
       ],
    "background": { 
        "scripts": ["script.js"]
    },
    "icons": { "16": "16x16.png",
        "48": "48x48.png",
       "128": "128x128.png" }
}